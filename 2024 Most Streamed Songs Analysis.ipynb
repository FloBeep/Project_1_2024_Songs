    "def extract_non_english(text):\n",
    "    # Define the regex pattern to match non-English characters\n",
    "    pattern = re.compile(r'[^\\p{IsLatin}\\s]', re.UNICODE)\n",
    "    # Find all non-English characters\n",
    "    non_english_chars = pattern.findall(text)\n",
    "    return ''.join(non_english_chars)"
